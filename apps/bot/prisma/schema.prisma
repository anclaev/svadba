datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated/client"
}

model Guest {
  id        Int      @id @default(autoincrement())
  login     String   @unique
  name      String?
  createdAt DateTime @default(now()) @map("created_at")
}
